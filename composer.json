{
    "name": "shintarosakata/laravel-repository",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shintaro.sakata",
            "email": "shintaro.sakata@leverages.jp"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "shintarosakata\\LaravelRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "shintarosakata\\LaravelRepository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "shintarosakata\\LaravelRepository\\LaravelRepositoryServiceProvider",
                "shintarosakata\\LaravelRepository\\ConsoleServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    }
}
